<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fitme.android.fitme">

  <uses-permission android:name="android.permission.INTERNET"/>
  <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_fitme_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".Register.RegisterActivity"> </activity>
        <activity android:name=".Login.LoginActivity" />

        <activity android:name=".FirstScreen.FirstScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
      <activity
          android:name=".MemberApp.memberAppActivity.MemberActivity"
          android:label="@string/title_activity_member"
          android:theme="@style/AppTheme.NoActionBar">
      </activity>
    </application>

</manifest>